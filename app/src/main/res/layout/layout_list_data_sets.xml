<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="cardData"
            type="com.example.finalprojectapp.data.model.adpters.LayoutDataSetView" />
        <variable
            name="displayDataSet"
            type="android.view.View.OnClickListener"/>

    </data>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/passwordMenu"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/credentials_recycler_View"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:theme="@style/AppTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/data_set_name"/>

    <TextView
        android:id="@+id/data_set_name"
        android:layout_width="167dp"
        android:layout_height="72dp"
        android:text="@{cardData.dataSetName}"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>

</layout>
